{
  "name": "swiftstay-backend",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "tsc && npx prisma generate && npx prisma migrate deploy",
    "start": "node dist/server/index.js",
